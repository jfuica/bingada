--with "/opt/COTS/gtkada-2019/lib/gnat/gtkada";
with "gtkada.gpr";

project bingada is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("bingada.adb");

   package Compiler is
      for Default_Switches ("ada") use
      ("-g",
       "-gnatwa",
       "-fstack-check",
       "-gnatVa",
       "-gnatU",
       "-gnatf",
       "-gnateE");

   end Compiler;

end bingada;
